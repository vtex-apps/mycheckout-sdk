{
  "name": "kuikpay-sdk",
  "version": "1.0.29",
  "description": "Made with create-react-library",
  "author": "",
  "license": "MIT",
  "repository": "/",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --css-modules _kuikpay-[name]__[local] --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --css-modules _kuikpay-[name]__[local] --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "deploy": "gh-pages -d example/build",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "lint:locales": "intl-equalizer",
    "build:dev": "npm publish --tag dev",
    "build:beta": "npm publish --tag beta"
  },
  "intl-equalizer": {
    "referenceLocale": "en",
    "localeDirectory": "messages"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "messages/*.json": [
      "yarn lint:locales"
    ]
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/enzyme": "^3.10.10",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^12.12.38",
    "@types/payment": "^2.1.3",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/react-intl": "^3.0.0",
    "@types/react-lottie": "^1.2.6",
    "@types/react-select": "^4.0.17",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vtex/intl-equalizer": "^2.5.0",
    "@vtex/prettier-config": "^0.3.5",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-config-vtex": "^12.8.11",
    "eslint-config-vtex-react": "^6.8.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@formatjs/intl-datetimeformat": "^4.2.3",
    "@reduxjs/toolkit": "^1.7.1",
    "@types/ramda": "^0.27.60",
    "classnames": "^2.3.1",
    "graphql": "^15.5.1",
    "payment": "^2.4.6",
    "ramda": "^0.27.1",
    "react-collapsible": "^2.8.3",
    "react-content-loader": "^6.2.0",
    "react-intl": "^5.20.10",
    "react-lottie": "^1.2.3",
    "react-redux": "^7.2.6",
    "react-select": "^4.3.1"
  }
}
